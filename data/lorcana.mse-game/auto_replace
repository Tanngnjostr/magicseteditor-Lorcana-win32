
auto replace:
	match: (C)
	replace: ©
auto replace:
	match: AE
	replace: Æ
	whole word: false
auto replace:
	match: TM
	replace: ™
	whole word: false
auto replace:
	match: --
	replace: —
auto replace:
	# note the spaces
	match:
		 - 
	replace:
		 — 



auto replace:
	match: ~
	replace: CARDNAME>
auto replace:
	match: '
	replace: ’
auto replace:
	match: ::
	replace: • 
auto replace:
	match: ->
	replace: •
